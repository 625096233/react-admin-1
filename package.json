    "react-hot-loader": "^3.0.0-beta.6",
    "sinon": "^2.0.0-pre.4",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "stylelint": "^7.6.0",
    "stylelint-config-standard": "^15.0.0",
    "url-loader": "^0.5.7",
