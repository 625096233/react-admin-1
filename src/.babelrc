{
  "env": {
    "development": {
      "presets": [
        ["latest", {
          "es2015": {
            "modules": false
          }
          }],
        "stage-0",
        "react"
      ],
      "plugins": [
        "transform-runtime",
        "add-module-exports", ["import", {
          "libraryName": "antd",
          "style": true,
        }]
      ]
    },
    "test": {
      "presets": [
        "latest",
        "stage-0",
        "react"
      ],
      "plugins": [
        "transform-runtime",
        "add-module-exports",
        "istanbul", ["import", {
          "libraryName": "antd",
          "style": true,
        }]
      ]
    },
    "production": {
      "presets": [
        "latest",
        "stage-0",
        "react"
      ],
      "plugins": [
        "transform-runtime",
        "add-module-exports", ["import", {
          "libraryName": "antd",
          "style": true,
        }]
      ]
    }
  }
}
